#include <Player.h>

Player::Player(int x,int y,int w,int h) {
	pos_x=x;
	pos_y=y;
	width=w;
	height=h;
}
Player::generateHIT(){
	LHIT=pos_x;
	RHIT=pos_x+width;
	THIT=pos_y;
	BHIT=pos_y+height;
}
Player::~Player() {
	// TODO Auto-generated destructor stub
}

